{
  "name": "pokeapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "polished": "^4.1.1",
    "react": "^17.0.2",
